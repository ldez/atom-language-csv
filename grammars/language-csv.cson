name: 'CSV'
scopeName: 'text.csv'
fileTypes: [
  'csv'
]
patterns: [
  name: 'constant.character.separator.csv'
  match: ','
,
#   name: 'constant.numeric'
#   # TODO
# ,
  name: 'string.cell.csv'
  match: '([^",]+)|(")((?:[^"]|"")*)(")'
  captures:
    2: name: 'variable.begin.csv'
    3:
      patterns: [
        match: '("")'
        captures:
          0: name: 'constant.character.escape.csv'
      ]
    4: name: 'variable.ending.csv'
]
